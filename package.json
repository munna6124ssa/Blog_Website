{
  "name": "blog-website",
  "version": "1.0.0",
  "description": "A modern full-stack blog website",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd clients && npm run dev",
    "build": "cd clients && npm run build",
    "start": "cd server && npm start",
    "install-server": "cd server && npm install",
    "install-client": "cd clients && npm install",
    "install-all": "npm run install-server && npm run install-client"
  },
  "keywords": [
    "blog",
    "react",
    "nodejs",
    "mongodb",
    "tailwindcss",
    "express"
  ],
  "author": "Munna Kumar",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
